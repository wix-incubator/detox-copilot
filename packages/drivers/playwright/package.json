{
    "name": "@pilot/playwright-driver",
    "version": "0.0.1",
    "description": "Playwright driver for pilot and copilot usage",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
      "dist"
    ],
    "publishConfig": {
      "registry": "https://registry.npmjs.org"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/wix-incubator/detox-copilot.git"
    },
    "scripts": {
      "build": "tsc && tsc-alias",
      "bump-version": "npm version patch && git push && git push --tags",
      "release": "npm run test && npm run bump-version && npm run build && npm publish",
      "lint": "eslint . --ext .ts",
      "lint:fix": "eslint . --ext .ts --fix",
      "type-check": "tsc --noEmit"
    },
    "bugs": {
      "url": "https://github.com/wix-incubator/detox-copilot/issues"
    },
    "dependencies": {
     "@pilot/core": "*"
    },

    "devDependencies": {
      "@playwright/test": "^1.50.0",
      "@typescript-eslint/eslint-plugin": "^6.21.0",
      "@typescript-eslint/parser": "^6.21.0",
      "eslint": "^8.56.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-prettier": "^5.1.3",
      "playwright": "^1.50.0",
      "prettier": "^3.2.5",
      "tsc-alias": "^1.8.10",
      "typescript": "^5.3.3"
    }
  }
  