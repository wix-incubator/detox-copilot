{
    "name": "@pilot/puppeteer-driver",
    "version": "0.0.1",
    "description": "Puppeteer driver for pilot and copilot usage",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
      "dist"
    ],
    "publishConfig": {
      "registry": "https://registry.npmjs.org"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/wix-incubator/detox-copilot.git"
    },
    "scripts": {
      "build": "tsc && tsc-alias",
      "test": "jest",
      "test:example": "jest examples/",
      "bump-version": "npm version patch && git push && git push --tags",
      "release": "npm run test && npm run bump-version && npm run build && npm publish",
      "lint": "eslint . --ext .ts",
      "lint:fix": "eslint . --ext .ts --fix",
      "type-check": "tsc --noEmit"
    },
    "bugs": {
      "url": "https://github.com/wix-incubator/detox-copilot/issues"
    },

    "dependencies": {
     "@pilot/core": "^0.0.37",
     "@pilot/drivers-utils": "^0.0.1",
     "puppeteer-core": "^20.8.0"
    },

    "devDependencies": {
      "@types/jest": "^29.5.0",
      "jest": "^29.5.0",
      "ts-jest": "^29.1.0",
      "typescript": "^5.0.0",
      "axios": "^1.7.9"
    }
  }
